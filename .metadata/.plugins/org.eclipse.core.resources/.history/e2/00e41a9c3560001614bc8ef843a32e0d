package java_20160812_Calculator_02;

public class Plus {
	
	public int plus(int num1, int num2){
		
		return num1 + num2;
		
	}

	public double plus(double num1, int num2){
		
		return num1 + num2;
		
	}
	
	public double plus(int num1, double num2){
		
		return num1 + num2;
		
	}
	
	public double plus(double num1, double num2){
		
		return num1 + num2;
		
	}

}
